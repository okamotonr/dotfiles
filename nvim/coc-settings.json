{
    "languageserver":{
        "haskell": {
            "command": "haskell-language-server-wrapper",
            "args": ["--lsp"],
            "rootPatterns": ["*.cabal", "stack.yml", "cabal.project", "package.yml", "hie.yml"],
            "filetypes": ["haskell", "lhaskell"]
        }
    },
    "python.linting.pylintEnabled": true
}
